<h1>Data Mapping Data Tindakan Medis</h1>

<table>
  <tr>
    <th>Kode RS</th>
    <th>Kode EJKBM</th>
    <th></th>
  </tr>
<tr>
    <td>
      <input type="text" size="7" onkeyup="searching(this.value, 'kode_rs')"/>
    </td>
    <td>
      <input type="text" size="7" onkeyup="searching(this.value, 'kode_ejkbm')"/>
    </td>
    <td colspan="9"></td>
  </tr>

  <tbody id="data_ajax_target">
    <%= render :partial => "mapping_data_tindakan_medis/data_index", :locals => {:mapping_data_tindakan_medis => @mapping_data_tindakan_medis} %>
  </tbody>

</table>

<script type="text/javascript">
  function searching(text, tipe){
    jQuery.ajax({
      url: "/mapping_data_tindakan_medis/index/new",
      dataType: "script",// "json",
      data: {
        authenticity_token: encodeURIComponent('<%= u form_authenticity_token %>'),
        param: text,
        tipe: tipe
      },
      beforeSend: function(data) {
        jQuery('#loading_body').toggle();
      },
      success: function(data) {
        //jQuery('#data').html(data);
      },
      complete: function(data) {
        jQuery('#loading_body').toggle();
      }
    });
  }
</script>