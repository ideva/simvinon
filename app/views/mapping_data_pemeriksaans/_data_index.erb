<% end %>

<tr style="background-color: transparent">
    <td colspan="100">
      <div class="digg_pagination">
        <div class="page_info">
          <%= raw page_entries_info @mapping_data_pemeriksaans %>
        </div>
        <%= will_paginate @mapping_data_pemeriksaans, :class => 'pagination ajax' %>
      </div>
    </td>
  </tr>
  <script type="text/javascript">
    ajaxifyPagination();
  </script>