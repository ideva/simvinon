<% @mapping_data_pemeriksaans.each do |mapping_data_pemeriksaan| %>
  <tr>
    <td><%= mapping_data_pemeriksaan.kode_pemeriksaan_rs %></td>
    <td><%= mapping_data_pemeriksaan.kode_pemeriksaan_ejkbm %></td>
    <%#= link_to 'Show', mapping_data_pemeriksaan %>
    <td><%= link_to 'Edit/Hapus', edit_mapping_data_pemeriksaan_path(mapping_data_pemeriksaan) %></td>
    <%#= link_to 'Destroy', mapping_data_pemeriksaan, :confirm => 'Are you sure?', :method => :delete %>
